<div class="container">
  <div class="row">
    <div class="panel panel-default profile col-md-6 col-md-offset-3">
      <div class="propic col-md-4">
        <%= image_tag @user.avatar.url,size:"100x110" %>
      </div>
      <div class="protab col-md-8">
        <table class="table">
          <tr>
            <td>Name</td><td><%= @user.fullname %></td>
  	      </tr>
  	      <tr>
            <td>Nick</td><td><%= @user.nick %></td>
          </tr>
  	      <tr>
  	        <td>Roll</td><td><%= @user.username %></td>
          </tr>
  	      <tr>
  	        <td>Room</td><td><%= @user.room %></td>
          </tr>
  	      <tr>
            <td>Hostel</td><td><%= @user.hostel %></td>
          </tr>
          <tr>
  	       <td>Email</td><td><%= @user.email %></td>
  	      </tr>
  	      <tr>
  	        <td>Phone</td><td><%= @user.contact %></td>
          </tr>
        </table>
        <%= link_to "Edit Profile",edit_user_path,class:"btn btn-primary" %><br> 
      </div>	
    </div>
  </div>
</div>
